{
  "framework": "sveltekit",
  "headers": [
    {
      "source": "/draft/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=0"
        }
      ]
    }
  ]
}